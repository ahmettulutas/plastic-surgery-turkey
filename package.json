{
  "name": "plastic-surgery-turkey",
  "version": "0.1.0",
  "private": true,
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "next dev -p 3005",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --max-warnings 0 --fix",
    "lint-staged": "lint-staged",
    "type-check": "tsc --noEmit",
    "postinstall": "husky install | chmod ug+x .husky/*",
    "format": "prettier --check --ignore-path .gitignore .",
    "format:fix": "prettier --write --ignore-path .gitignore ."
  },
  "dependencies": {
    "next": "14.2.14",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.14",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.1",
    "eslint-plugin-boundaries": "^4.2.2",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "typescript": "^5"
  }
}
